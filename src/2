<?php
declare(strict_types=1);
namespace LanguageTools;

enum Status: string
{
    case DRAFT = 'draft';
    case PUBLISHED = 'published';
    case ARCHIVED = 'archived';
}

enum ClassID: string {

   case   PONS = "p";
   case   SYSTRANS = "i";
   case   AZURE = "a";
   case   IBM = "i";
   case   YANDEX = "y";
   case   DEEPL = "d";
}
